class Solution {
public:
    string kthDistinct(vector<string>& arr, int k) {
        unordered_map<string, int> wordCount;

        
        for (const string& s : arr) {
            wordCount[s]++;
        }

        
        int count = 0;
        for (const string& s : arr) {
            if (wordCount[s] == 1) {
                count++;
                if (count == k) {
                    return s;
                }
            }
        }

        return "";
    }
};

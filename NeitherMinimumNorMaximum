class Solution {
public:
    int findNonMinOrMax(vector<int>& nums) {
        sort(nums.begin(),nums.end(), greater<int>());
        for(int  i =0;i<nums.size() ; i++){
            if( i != 0 && i != (nums.size()-1)){
                return nums[i];
            }
        }
        return -1;
    }
};

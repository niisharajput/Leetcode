class Solution {
public:
    int sumOfUnique(vector<int>& nums) {
        unordered_map<int,int> freqMap;

        for(int  num : nums){
            int&frequency = freqMap[num];
            frequency++;
        }

        int sum = 0;
        for(const auto & pair : freqMap){
            if(pair.second == 1){
                sum = sum + pair.first;
            }
        }

        return sum;

        
    }
};
